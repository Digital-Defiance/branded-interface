{
  "name": "@digitaldefiance/branded-interface",
  "version": "0.0.4",
  "description": "Runtime-identifiable interface-like types for TypeScript with zero runtime overhead",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Digital-Defiance/branded-interface.git"
  },
  "keywords": [
    "typescript",
    "interface",
    "branded",
    "type-safe",
    "runtime"
  ],
  "author": "Digital Defiance, Jessica Mulein",
  "bugs": {
    "url": "https://github.com/Digital-Defiance/branded-interface/issues"
  },
  "homepage": "https://github.com/Digital-Defiance/branded-interface#readme",
  "scripts": {
    "build": "NODE_ENC=development npx nx build branded-interface --configuration=development",
    "build:prod": "NODE_ENV=production npx nx build branded-interface --configuration=production",
    "test": "npx nx test branded-interface",
    "lint": "npx nx lint branded-interface",
    "publish:public": "yarn build && npm publish --access public"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@nx/eslint": "^22.5.1",
    "@nx/jest": "22.4.2",
    "@nx/js": "22.4.2",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "eslint": "^9.0.0",
    "fast-check": "^4.5.3",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "nx": "22.4.2",
    "prettier": "~3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.56.0"
  },
  "packageManager": "yarn@4.12.0",
  "nx": {
    "name": "branded-interface",
    "sourceRoot": "src",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "outputs": [
          "{projectRoot}/dist"
        ],
        "options": {
          "commands": [
            "swc src -d dist/esm --config-file .swcrc --strip-leading-paths",
            "swc src -d dist/cjs --config-file .swcrc.cjs --strip-leading-paths -C module.type=commonjs",
            "tsc -p tsconfig.lib.json",
            "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json"
          ],
          "cwd": "{projectRoot}",
          "parallel": false
        }
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "jest.config.cts",
          "passWithNoTests": true
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "options": {
          "lintFilePatterns": [
            "src/**/*.ts"
          ]
        }
      },
      "publish-public": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npm publish --access public"
        },
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "engines": {
    "node": ">=18"
  }
}
